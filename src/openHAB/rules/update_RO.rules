rule RO_EG_SU
	when
		Item RO_EG_SU received command
	then
    var String message="!w!RO_EG_SU!0$!w!RO_EG_SU_ON!0$!w!RO_EG_SU_UD!0$"
	if (receivedCommand==STOP) {postUpdate(RO_EG_SU, 50) }
	if (receivedCommand==UP) {message="!w!RO_EG_SU!1$"}
	if (receivedCommand==DOWN) {message="!w!RO_EG_SU!-1$"}
    sendCommand(RS485_Gateway_out, message)
    println("rs485 out " + message)
end

